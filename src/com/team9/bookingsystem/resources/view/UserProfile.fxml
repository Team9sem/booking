<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<!--by Nima, Olle-->

<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane prefHeight="700.0"  prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.team9.bookingsystem.Controllers.ProfileController">
   <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <top>
         <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            </columnConstraints>
            <rowConstraints>
               <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
               <HBox alignment="CENTER_LEFT" prefWidth="310.0" GridPane.columnIndex="1">
                  <children>
                     <Label fx:id="profileTitle" alignment="CENTER_LEFT" prefHeight="35.0" prefWidth="400.0" styleClass="upBigLabels" text="User Profile">
                        <font>
                           <Font size="50.0" />
                        </font>
                     </Label>
                  </children>
               </HBox>
               <HBox alignment="CENTER_LEFT" prefWidth="400.0">
                  <children>
                     <Button fx:id="backToBookings" mnemonicParsing="false" onAction="#previousPage" styleClass="search-button" text="Previous Page" />
                  </children>
                  <GridPane.margin>
                     <Insets bottom="5.0" left="5.0" top="5.0" />
                  </GridPane.margin>
               </HBox>
            </children>
         </GridPane>
      </top>
      <left>
         <VBox prefHeight="300.0" prefWidth="300.0" spacing="10.0" styleClass="upVbox" BorderPane.alignment="TOP_LEFT">
            <children>
               <HBox alignment="CENTER" />
               <HBox>
                  <children>
                     <ImageView fx:id="imageView" fitHeight="159.0" fitWidth="193.0" pickOnBounds="true" preserveRatio="false">
                        <image>
                           <Image url="@booking/src/com/team9/bookingsystem/resources/img/Death_to_stock_communicate_hands_2.jpg" />
                        </image>
                     </ImageView>
                     <HBox alignment="CENTER">
                        <children>
                           <Button fx:id="ppButton" mnemonicParsing="false" onAction="#handle" styleClass="search-button" text="Upload Picture" />
                        </children>
                        <HBox.margin>
                           <Insets left="5.0" />
                        </HBox.margin>
                     </HBox>
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox alignment="TOP_CENTER" prefHeight="30.0" prefWidth="162.0">
                  <children>
                     <Label fx:id="userInfo" prefHeight="61.0" prefWidth="160.0" styleClass="upBigLabels" text="User Info">
                        <font>
                           <Font size="25.0" />
                        </font>
                        <HBox.margin>
                           <Insets />
                        </HBox.margin>
                     </Label>
                  </children>
                  <VBox.margin>
                     <Insets bottom="20.0" />
                  </VBox.margin>
               </HBox>
               <HBox prefHeight="1.0">
                  <children>
                     <Label prefWidth="212.0" styleClass="upLabels" text="Username:" />
                     <Label fx:id="userName" prefWidth="281.0" styleClass="upLabels" text="Username input" />
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox prefHeight="10.0" prefWidth="299.0">
                  <children>
                     <Label prefHeight="10.0" prefWidth="306.0" styleClass="upLabels" text="Password:" />
                     <Label fx:id="passWord" prefWidth="378.0" styleClass="upLabels" text="Password input" />
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox prefHeight="10.0" prefWidth="299.0">
                  <children>
                     <Label prefWidth="299.0" styleClass="upLabels" text="Firstname:" />
                     <Label fx:id="firstName" prefWidth="370.0" styleClass="upLabels" text="Firstname input" />
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox prefHeight="10.0" prefWidth="299.0">
                  <children>
                     <Label prefWidth="301.0" styleClass="upLabels" text="Lastname:" />
                     <Label fx:id="lastName" prefWidth="374.0" styleClass="upLabels" text="Lastname input" />
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox prefHeight="10.0" prefWidth="299.0">
                  <children>
                     <Label prefWidth="309.0" styleClass="upLabels" text="Adress:" />
                     <Label fx:id="adress" prefWidth="385.0" styleClass="upLabels" text="Adress input" />
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox prefHeight="10.0" prefWidth="299.0">
                  <children>
                     <Label prefWidth="304.0" styleClass="upLabels" text="Zipcode:" />
                     <Label fx:id="zipCode" prefWidth="380.0" styleClass="upLabels" text="Zipcode input" />
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox prefHeight="10.0" prefWidth="299.0">
                  <children>
                     <Label prefWidth="299.0" styleClass="upLabels" text="SSN:" />
                     <Label fx:id="ssn" prefWidth="375.0" styleClass="upLabels" text="SSN input">
                        <HBox.margin>
                           <Insets />
                        </HBox.margin></Label>
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" />
                  </VBox.margin>
               </HBox>
            </children>
            <BorderPane.margin>
               <Insets left="5.0" />
            </BorderPane.margin>
         </VBox>
      </left>
      <right>
         <VBox prefHeight="221.0" prefWidth="500.0" styleClass="upBookings" BorderPane.alignment="TOP_LEFT">
            <children>
               <HBox VBox.vgrow="SOMETIMES">
                  <children>
                     <Label styleClass="bookingsText" text="Current Bookings" />
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox VBox.vgrow="SOMETIMES">
                  <children>
                     <TableView fx:id="currentBookings" prefHeight="400.0" prefWidth="500.0">
                     </TableView>

                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox VBox.vgrow="SOMETIMES">
                  <children>
                     <Label styleClass="bookingsText" text="Booking History" />
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
               </HBox>
               <HBox VBox.vgrow="SOMETIMES">
                  <children>
                     <TableView fx:id="bookingHistory" editable="true" prefHeight="400.0" prefWidth="500.0">
                     </TableView>
                  </children>
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
               </HBox>
            </children>
            <BorderPane.margin>
               <Insets />
            </BorderPane.margin>
         </VBox>
      </right>
   </BorderPane>
   <HBox fx:id="progressLayer" style="-fx-background-color: rgba(255, 255, 255, 0.21)" AnchorPane.topAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.bottomAnchor="0.0" alignment="CENTER">
      <children>
         <ProgressIndicator prefHeight="150" prefWidth="150"></ProgressIndicator>
      </children>
   </HBox>
</AnchorPane>